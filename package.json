{
  "name": "quickfetch",
  "version": "1.0.0-alpha.3",
  "description": "a fetch-based HTTP request tool like axios",
  "main": "dist/QuickFetch.js",
  "scripts": {
    "test": "jest",
    "doc": "jsdoc2md src/index.ts src/MiddlewareHolder.ts --template README.hbs > README.md",
    "build": "shell-exec --colored-output \"rm -rf dist\" \"microbundle -i src/index.ts -o dist/QuickFetch.js --no-pkg-main -f umd\"",
    "mergets": "node merge-ts.js"
  },
  "pre-commit": [
    "test"
  ],
  "keywords": [
    "fetch"
  ],
  "author": "tonylua@sina.com",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/tonylua/QuickFetch.git"
  },
  "sideEffects": false,
  "dependencies": {
    "lodash-es": "^4.17.15",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/plugin-proposal-pipeline-operator": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.7",
    "@types/lodash-es": "^4.17.3",
    "@types/qs": "^6.9.4",
    "@types/webpack-env": "^1.15.2",
    "babel-jest": "^26.1.0",
    "file-saver": "^2.0.2",
    "jest": "^26.1.0",
    "jest-fetch-mock": "^3.0.3",
    "jsdoc-to-markdown": "^6.0.1",
    "klaw-sync": "^6.0.0",
    "microbundle": "^0.12.3",
    "pre-commit": "^1.2.2",
    "shell-executor": "^6.0.1",
    "ts-jest": "^26.1.3",
    "tslib": "^2.0.0",
    "typescript": "^3.9.7"
  }
}
